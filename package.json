{
  "name": "woof",
  "version": "0.1.0",
  "description": "Agent orchestrator CLI with LLM chat interaction",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "woof": "dist/index.js"
  },
  "scripts": {
    "build": "bun build src/index.tsx --outdir dist --target bun",
    "build:cli": "bun build src/index.tsx --compile --outfile dist/woof",
    "build:server": "bun build src/server/index.ts --compile --outfile dist/woof-server",
    "build:all": "bun run build:cli && bun run build:server",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "dev": "bun src/index.tsx",
    "start": "bun src/index.tsx",
    "server": "bun src/server/index.ts",
    "server:dev": "bun --watch src/server/index.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^0.0.39",
    "@ai-sdk/openai": "^0.0.39",
    "@anthropic-ai/sdk": "^0.40.0",
    "@inquirer/prompts": "^4.0.0",
    "ai": "^3.0.0",
    "chalk": "^5.6.2",
    "conf": "^12.0.0",
    "ink": "^4.4.1",
    "ink-select-input": "^5.0.0",
    "ink-text-input": "^5.0.1",
    "react": "^18.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "bun-types": "^1.0.0",
    "ink-testing-library": "^4.0.0",
    "react-devtools-core": "^7.0.1",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "",
  "license": "MIT"
}
